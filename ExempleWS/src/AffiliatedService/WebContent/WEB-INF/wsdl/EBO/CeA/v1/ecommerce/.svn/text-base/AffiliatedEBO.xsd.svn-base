<schema elementFormDefault="qualified" 
	xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.cea.com.br/EBS/CEA/ecommerce/affiliated/v1" 
	xmlns:tns="http://www.cea.com.br/EBS/CEA/ecommerce/affiliated/v1">
	
	<complexType name="Affiliated">
		<sequence>
			<element name="Matricula" type="long">
				<annotation>
					<documentation>
						<![CDATA[Identificador do associado]]>
					</documentation>
				</annotation>
			</element>
			<element name="CPF" type="string">
				<annotation>
					<documentation>
						<![CDATA[CPF ou CNPJ]]>
					</documentation>
				</annotation>
			</element>
			<element name="Status">
				<annotation>
			      <documentation>Ativo - Associado esta ativo / Inativo - Associado esta desligado</documentation>
			    </annotation>
				<simpleType>
				    <restriction base="string">
				      <enumeration value="ATIVO"/>
				      <enumeration value="INATIVO"/>
				    </restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="Affiliates">
		<sequence>
			<element name="Affiliated" type="tns:Affiliated" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	
</schema>